{
    "name": "naykel/docit",
    "description": "Beautiful documentation system for Laravel applications with markdown and syntax highlighting",
    "keywords": [
        "laravel",
        "documentation",
        "markdown",
        "docs",
        "syntax-highlighting"
    ],
    "type": "library",
    "homepage": "https://naykel.com.au",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Watts",
            "email": "nathan@naykel.com.au",
            "homepage": "https://naykel.com.au"
        }
    ],
    "require": {
        "php": "^8.2",
        "graham-campbell/markdown": "^16.0",
        "torchlight/torchlight-laravel": "^0.6.1"
    },
    "require-dev": {
        "laravel/pint": "^1.25",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Naykel\\Docit\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "autoload": {
        "psr-4": {
            "Naykel\\Docit\\": "src/"
        }
    },
    "scripts": {
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naykel\\Docit\\DocitServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}